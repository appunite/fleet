[Unit]
Description=Jenkins volume Service

[Service]
TimeoutStartSec=0
ExecStartPre=-/usr/bin/docker kill %n
ExecStartPre=-/usr/bin/docker rm %n
ExecStartPre=/usr/bin/docker pull busybox
ExecStart=/usr/bin/docker run --volume /jenkins --name %n busybox  /bin/sh -c "while true; do echo 'Server on fire!'; sleep 30; done"
ExecStop=/usr/bin/docker stop %n

[X-Fleet]
MachineOf=squid.service